{
  "name": "nba-schedule",
  "version": "1.1.0",
  "description": "A schedule for the 2021-22 NBA season",
  "scripts": {
    "watch:sass": "sass --watch _src/assets/scss:_site/assets/css --no-source-map",
    "build:sass": "sass _src/assets/scss:_site/assets/css --no-source-map",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/peruvianidol/nba-schedule.git"
  },
  "keywords": [],
  "author": "Mike Aparicio <peruvianidol@gmail.com> (http://mikeaparicio.com/)",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "eleventy-plugin-svg-sprite": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.35.1"
  }
}
